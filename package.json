  },
  "homepage": "https://github.com/Devil7-Softwares/devil7softwares.in#readme",
  "devDependencies": {
    "@types/parcel-bundler": "^1.12.1",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6",
    "parcel": "^1.12.4",
    "parcel-bundler": "^1.12.4",
    "typescript": "^4.0.3"
  },
