{
  "name": "devil7softwares.in",
  "version": "0.0.1",
  "description": "Front & Backend for Devil7Softwares Website",
  "main": "index.js",
  "scripts": {
    "start": "concurrently --kill-others \"tsc -p src/server -w\" \"nodemon --ignore dist/public/* --watch dist dist/index.js\"",
    "build": "npm run clean && tsc -p src/server && parcel build --out-dir dist/public src/client/index.html",
    "clean": "rm -rf dist"
  },
